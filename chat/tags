!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
CBUF_SIZE_DEFAULT	chat.h	6;"	d
CC	Makefile	/^CC=gcc$/;"	m
CFLAGS	Makefile	/^CFLAGS=-std=c99 $(DBGFLAG) -Wall$/;"	m
DBGFLAG	Makefile	/^DBGFLAG=-ggdb $/;"	m
LDFLAGS	Makefile	/^LDFLAGS=$(DBGFLAG) $/;"	m
LDLIBS	Makefile	/^LDLIBS=-lpthread -lncurses$/;"	m
OBJECTS	Makefile	/^OBJECTS=$(SOURCES:.c=.o)$/;"	m
SOURCES	Makefile	/^SOURCES=chat.c$/;"	m
TARGET	Makefile	/^TARGET=chat$/;"	m
_XOPEN_SOURCE	chat.c	1;"	d	file:
__CHAT_H__	chat.h	2;"	d
bsize	chat.h	/^	int bsize;$/;"	m	struct:chat_buffer
cbuf_add	chat.c	/^void cbuf_add(struct chat_buffer *cbuf, const char *msg){$/;"	f
cbuf_destroy	chat.c	/^void cbuf_destroy(struct chat_buffer *cbuf){$/;"	f
cbuf_init	chat.c	/^void cbuf_init(struct chat_buffer *cbuf){$/;"	f
chat_buffer	chat.h	/^struct chat_buffer{$/;"	s
chat_buffer	chat.h	/^} chat_buffer;$/;"	v	typeref:struct:chat_buffer
lock	chat.h	/^	pthread_mutex_t lock;$/;"	m	struct:chat_buffer
main	chat.c	/^int main(int argc, char *argv[]){$/;"	f
msgbuf	chat.h	/^	char **msgbuf; $/;"	m	struct:chat_buffer
nmsg	chat.h	/^	int nmsg;$/;"	m	struct:chat_buffer
usage	chat.c	/^void usage(char *argv[]) {$/;"	f
