CBUF_SIZE_DEFAULT	client.h	6;"	d
CC	Makefile	/^CC=gcc$/;"	m
CFLAGS	Makefile	/^CFLAGS=-std=c99 $(DBGFLAG) -Wall$/;"	m
DBGFLAG	Makefile	/^DBGFLAG=-ggdb $/;"	m
LDFLAGS	Makefile	/^LDFLAGS=$(DBGFLAG) $/;"	m
LDLIBS	Makefile	/^LDLIBS=-lpthread -lncurses$/;"	m
OBJECTS	Makefile	/^OBJECTS=$(SOURCES:.c=.o)$/;"	m
SOURCES	Makefile	/^SOURCES=client.c ..\/common\/chat.c$/;"	m
TARGET	Makefile	/^TARGET=client$/;"	m
_XOPEN_SOURCE	client.c	1;"	d	file:
__CLIENT_H__	client.h	2;"	d
bsize	client.h	/^	int bsize;$/;"	m	struct:chat_buffer
cbuf_add	client.c	/^void cbuf_add(struct chat_buffer *cbuf, const char *msg){$/;"	f
cbuf_destroy	client.c	/^void cbuf_destroy(struct chat_buffer *cbuf){$/;"	f
cbuf_init	client.c	/^void cbuf_init(struct chat_buffer *cbuf){$/;"	f
chat_buffer	client.h	/^struct chat_buffer{$/;"	s
chat_buffer	client.h	/^} chat_buffer;$/;"	v	typeref:struct:chat_buffer
diep	client.c	/^void diep(char *msg){$/;"	f
lock	client.h	/^	pthread_mutex_t lock;$/;"	m	struct:chat_buffer
main	client.c	/^int main(int argc, char *argv[]){$/;"	f
msgbuf	client.h	/^	char **msgbuf; $/;"	m	struct:chat_buffer
nmsg	client.h	/^	int nmsg;$/;"	m	struct:chat_buffer
usage	client.c	/^void usage(char *argv[]) {$/;"	f
